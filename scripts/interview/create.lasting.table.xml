<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <changeSet id="before_create_lasting_table" author="shuffle">
        <tagDatabase tag="before_create_lasting_table"/>
    </changeSet>
    <changeSet id="create_lasting_table" author="shuffle">
        <createTable tableName="LASTING">
            <column name ="ID" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="LASTING" type="INTEGER">
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>



